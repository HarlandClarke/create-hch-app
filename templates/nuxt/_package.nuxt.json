{
  "name": "<%= choices.appName %>",
  "version": "1.0.0",
  "description": "<%= choices.description %>",
  "author": "<%= choices.author %>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate"<% if (choices.eslint) { %>,
    "lint": "eslint --ext .js, .vue --ignore-path .gitignore .",
    "precommit": "npm run lint"<% } %>
  },
  "dependencies": {
    "nuxt": "~2.0"<% if (choices.http === 'axios') %>,
    "@nuxtjs/axios": "^5.0.0"<% } else if (choices.http === 'superagent') { %>,
    "superagent": "^4.0.0-beta.5"<% } %><% if (choices.framework === 'foundation') { %>,
    "foundation-sites": "~6.5",
    "jquery": ">=2.2.0",
    "what-input": ">=4.1.0"<% } else if (choices.framework === 'bootstrap') { %>,
    "bootstrap-vue": "~2.0",
    "bootstrap": "~4.1.3",
    "jquery": ">=1.9.3",<% } else if (choices.framework === 'vuetify') { %>,
    "vuetify": "~1.3.0",
    "vuetify-loader": "~1.0.5"<% } else if (choices.framework === 'quasar') { %><% } %>
  },
  "devDependencies": {
    "nodemon": "^1.11.0"<% if (eslint) { %>,
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-loader": "^2.1",
    "eslint-plugin-vue": "^5.0.0-0"<% } %><% if (framework === 'vuetify') { %>,
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2"<% } %>,
    "node-sass": "^4.9.4",
    "sass-loader": "^7.1.0"
  }
}
