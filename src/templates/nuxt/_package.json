{
  "name": "<%= appName %>",
  "version": "1.0.0",
  "description": "<%= description %>",
  "author": "<%= author %>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate"<% if (eslint) { %>,
    "lint": "eslint --ext .js, .vue --ignore-path .gitignore .",
    "precommit": "npm run lint"<% } %>
  },
  "dependencies": {
    "nuxt": "~2.0",
    "nuxt-i18n": "^5.4.4"<% if (http === 'axios') { %>,
    "@nuxtjs/axios": "^5.0.0"<% } else if (http === 'superagent') { %>,
    "superagent": "^4.0.0-beta.5"<%
    } %><% if (framework === 'foundation') {%>,
    "foundation-sites": "~6.5",
    "jquery": ">=2.2.0",
    "what-input": ">=4.1.0"<% } else if (framework === 'bootstrap') { %>,
    "bootstrap-vue": "~2.0",
    "bootstrap": "~4.1.3",
    "jquery": ">=1.9.3",<% } else if (framework === 'vuetify') { %>,
"vuetify": "~1.3.0"<%} else if (framework === 'quasar') {%><%} %><% if (validator === 'vuelidate') {%>,
"vuelidate": "^0.7.4"<%
} %>
  },
  "devDependencies": {
"nodemon": "^1.11.0",
"jest": "^23.6.0",
"standard": "^12.0.1"<% if (standard_version) {%>,
"standard-version": "^4.4.0"<%} %><% if (eslint) {%>,
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-loader": "^2.1",
"eslint-plugin-vue": "^5.0.0-0"<%} %><% if (lang === 'ts') {%>,
"typescript": "^3.1.5",
"ts-loader": "^5.3.0"<%} else if (lang === 'es2015') {%>,
"@babel/preset-es2015": "^7.0.0-beta.53"<%} else if (lang === 'es2016') {%>,
"@babel/preset-es2016": "^7.0.0-beta.53"<%} else if (lang === 'es2017') {%>,
"@babel/preset-es2017": "^7.0.0-beta.53"<%} %><% if (framework === 'vuetify' || css === 'stylus') {
%>,
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2"<% } %>,
"css-loader": "^1.0.1"<% if (css === 'scss') {%>,
    "node-sass": "^4.9.4",
"sass-loader": "^7.1.0"<%} else if (css === 'less') {%>,
"less": "^3.0.4",
"less-loader": "^4.1.0"<%} %><% if (framework === 'vuetify') {%>,
"vuetify-loader": "~1.0.5"<%
} %>
  }
}
