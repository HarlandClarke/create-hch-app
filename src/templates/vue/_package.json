{
  "name": "<%= appName %>",
  "version": "1.0.0",
  "description": "<%= description %>",
  "author": "<%= author %>",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build"<% if (eslint) { %>,
    "lint": "vue-cli-service lint"<% } %>
  },
  "dependencies": {<% if (http === 'axios') { %>,
    "axios": "~0.18",
    "vue-axios": "~2"<% } else if (http === 'superagent') { %>,
    "superagent": "^4.0.0-beta.5"<% } %><% if (framework === 'foundation') { %>,
    "foundation-sites": "~6.5",
    "jquery": ">=2.2.0",
    "what-input": ">=4.1.0"<% } else if (framework === 'bootstrap') { %>,
    "bootstrap-vue": "~2.0",
    "bootstrap": "~4.1.3",
    "jquery": ">=1.9.3",<% } else if (framework === 'vuetify') { %>,
    "vuetify": "~1.3.0",
    "vuetify-loader": "~1.0.5"
    <% } else if (framework === 'quasar') { %><% } %>
  },
  "devDependencies": {
    "vue": "^2.5.17",
    "@vue/cli-plugin-babel": "^3.0.5",
    "@vue/cli-plugin-eslint": "^3.0.5",
    "@vue/cli-service": "^3.0.5",
    "vue-template-compiler": "^2.5.17"<% if (eslint) { %>,
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0-0"<% } %><% if (lang === 'ts') { %>,
    "typescript": "^3.1.5",
    "ts-loader": "^5.3.0"<% } else if (lang === 'es2015') { %>,
    "@babel/preset-es2015": "^7.0.0-beta.53"<% } else if (lang === 'es2016') { %>,
    "@babel/preset-es2016": "^7.0.0-beta.53"<% } else if (lang === 'es2017') { %>,
    "@babel/preset-es2017": "^7.0.0-beta.53"<% } else { %>,
    "@babel/preset-env": "^7.1.0"<% } %><% if (framework === 'vuetify') { %>,
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2"<% } %>
  }<% if (eslint) { %>,
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  }<% } %>,
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
